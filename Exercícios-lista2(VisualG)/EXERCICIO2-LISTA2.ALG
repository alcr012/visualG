Algoritmo "exercicio2-lista2"
Var
a,b,soma,sub,divi,multi:real


Inicio
escreva("Digite um número: ")
leia(a)
escreva("Digite outro número: ")
leia(b)

soma <- a+b
sub <- a-b
divi <- a/b
multi <- a*b
escreval()
escreval("Resultados foram: ")
escreval("SOMA: ",soma)
escreval("SUBTRAÇÃO: ",sub)
escreval("DIVISÃO: ",divi)
escreval("MULTIPLICAÇÃO: ",multi)
Fimalgoritmo